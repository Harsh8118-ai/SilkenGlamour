[build]
  base    = "Frontend"          # Path to your frontend folder
  publish = "Frontend/dist"     # Or 'Frontend/build' for Create React App
  command = "npm run build"     # Build command to generate the production build

[[redirects]]
  from = "/*"
  to = "/index.html"
  status = 200

[build.environment]
 NODE_VERSION = "20.19.0"
